package lab;


public class loop {
    public static void main(String[] args) {
    int num=7;
   if(num<0){
    System.out.println("The number is positive:");
   }
   else if(num >0){
        System.out.println("The number is Negative:");
   }
   else{
       System.out.println("Otherwise the number is zero:");
   }
    }
}
