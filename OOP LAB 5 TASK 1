package student;

public class Student {
    public String name;
    public String city;
    public int age;

    public static void main(String[] args) {
        Student s1 = new Student();
        s1.name = "Aleena";
        s1.city = "Islamabad";
        s1.age = 18;

        System.out.println("Student name is: " + s1.name);
        System.out.println("Student city is: " + s1.city);
        System.out.println("Student age is: " + s1.age);

        Student s2 = new Student();
        s2.name = "Bisma";
        s2.city = "Lahore";
        s2.age = 20;

        System.out.println("Student name is: " + s2.name);
        System.out.println("Student city is: " + s2.city);
        System.out.println("Student age is: " + s2.age);
    }
}
